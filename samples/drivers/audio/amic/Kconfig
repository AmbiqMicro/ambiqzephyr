source "Kconfig.zephyr"

config SAMPLE_FREQ
	int "Sample rate"
	default 16000
	help
	  Sample frequency of the system.

config AMIC_CHANNELS
	int "Number of AMIC channels"
	default 1
	help
	  Count of AMIC channels to capture and stream.
