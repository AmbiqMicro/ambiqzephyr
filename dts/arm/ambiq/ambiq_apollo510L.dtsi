			status = "disabled";
		};

		pinctrl: pin-controller@GPIO_BASE_NAME {
			compatible = "ambiq,apollo5-pinctrl";
			reg = <GPIO_REG_BASE GPIO_REG_SIZE>;
			#address-cells = <1>;
			#size-cells = <0>;

			gpio: gpio@GPIO_BASE_NAME {
				compatible = "ambiq,gpio";
				gpio-map-mask = <0xffffffe0 0xffffffc0>;
				gpio-map-pass-thru = <0x1f 0x3f>;
				gpio-map = <
					0x00 0x0 &gpio0_31 0x0 0x0
					0x20 0x0 &gpio32_63 0x0 0x0
					0x40 0x0 &gpio64_95 0x0 0x0
					0x60 0x0 &gpio96_119 0x0 0x0
				>;
				reg = <GPIO_REG_BASE>;
				#gpio-cells = <2>;
				#address-cells = <1>;
				#size-cells = <0>;
				ranges;

				gpio0_31: gpio0_31@0 {
					compatible = "ambiq,gpio-bank";
					gpio-controller;
					#gpio-cells = <2>;
					reg = <0>;
					interrupts = <56 0>;
					status = "disabled";
				};

				gpio32_63: gpio32_63@80 {
					compatible = "ambiq,gpio-bank";
					gpio-controller;
					#gpio-cells = <2>;
					reg = <0x80>;
					interrupts = <57 0>;
					status = "disabled";
				};

				gpio64_95: gpio64_95@100 {
					compatible = "ambiq,gpio-bank";
					gpio-controller;
					#gpio-cells = <2>;
					reg = <0x100>;
					interrupts = <58 0>;
					status = "disabled";
				};

				gpio96_119: gpio96_119@180 {
					compatible = "ambiq,gpio-bank";
					gpio-controller;
					#gpio-cells = <2>;
					reg = <0x180>;
					interrupts = <59 0>;
					ngpios = <24>;
					status = "disabled";
				};
			};
		};

		mbox: ipc: mbox@40034000 {
			compatible = "ambiq,mbox-ipc";
			reg = <0x40034000 0x400>;
			tx-mask = <0x0000ffff>;
			rx-mask = <0x0000ffff>;
			interrupts = <17 0>, <18 0>;
			#mbox-cells = <1>;
			status = "disabled";
		};
	};

	ipc {
		ipc0: ipc0 {
			compatible = "zephyr,ipc-openamp-static-vrings";
			/* The channel 0 and 1 of mbox are used for the IPC message
			 * transmit notification and receive interrupt between
			 * application core and radio core.
			 */
			mboxes = <&mbox 0>, <&mbox 1>;
			mbox-names = "tx", "rx";
			role = "host";
			zephyr,buffer-size = <704>;
			status = "disabled";
		};
	};

	mbox-consumer {
		compatible = "vnd,mbox-consumer";
		/* The channel 2 and 3 of mbox are used for vendor specific request
		 * and response usage between application core and radio core.
		 */
		mboxes = <&mbox 2>, <&mbox 3>;
		mbox-names = "tx", "rx";
	};
};

&nvic {
	arm,num-irq-priority-bits = <4>;
};
