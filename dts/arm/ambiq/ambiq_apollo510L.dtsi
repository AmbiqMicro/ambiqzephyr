/ {
	clocks {
		xo48m_xtal: xo48m_xtal {
			compatible = "fixed-clock";
			#clock-cells = <0>;
		};

		xo48m_ext: xo48m_ext {
			compatible = "fixed-clock";
			#clock-cells = <0>;
		};

		xo32k_xtal: xo32k_xtal {
			compatible = "fixed-clock";
			#clock-cells = <0>;
		};

		xo32k_ext: xo32k_ext {
			compatible = "fixed-clock";
			#clock-cells = <0>;
		};

		extrefclk: extrefclk {
			compatible = "fixed-clock";
			#clock-cells = <0>;
		};
	};

	performance-states {
		compatible = "zephyr,pstates";

		pstate_hp: pstate_hp {
			compatible = "ambiq,pstate";
			load-threshold = <80>;
			ambiq,perf-mode = <AMBIQ_POWER_MODE_HIGH_PERFORMANCE_2>;
		};

		pstate_hp1: pstate_hp1 {
			compatible = "ambiq,pstate";
			load-threshold = <60>;
			ambiq,perf-mode = <AMBIQ_POWER_MODE_HIGH_PERFORMANCE>;
		};

		pstate_lp: pstate_lp {
			compatible = "ambiq,pstate";
			load-threshold = <0>;
			ambiq,perf-mode = <AMBIQ_POWER_MODE_LOW_POWER>;
		};
	};

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu0: cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-m55";
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <1>;
			cpu-power-states = <&idle &suspend_to_ram &suspend_to_disk>;

			itm: itm@e0000000 {
				compatible = "arm,armv8m-itm";
				reg = <0xe0000000 0x1000>;
				swo-ref-frequency = <DT_FREQ_M(48)>;
			};

			mpu: mpu@e000ed90 {
				compatible = "arm,armv8m.1-mpu";
				reg = <0xe000ed90 0x40>;
			};
		};

		power-states {
			idle: idle {
				compatible = "zephyr,power-state";
				power-state-name = "suspend-to-idle";
				min-residency-us = <1000>;
				exit-latency-us = <50>;
			};

			suspend_to_ram: suspend_to_ram {
				compatible = "zephyr,power-state";
				power-state-name = "suspend-to-ram";
				min-residency-us = <1200>;
				exit-latency-us = <300>;
			};

			suspend_to_disk: suspend_to_disk {
				compatible = "zephyr,power-state";
				power-state-name = "suspend-to-disk";
				min-residency-us = <1500>;
				exit-latency-us = <400>;
			};
		};
	};

	/* MRAM region */
	flash: flash-controller@MRAM_BASE_NAME {
		compatible = "ambiq,flash-controller";
		reg = <MRAM_BASE_ADDR MRAM_MAX_SIZE>;

		#address-cells = <1>;
		#size-cells = <1>;
		/* Flash region */
		flash0: flash@MRAM_BASE_NAME {
			compatible = "soc-nv-flash";
			reg = <MRAM_BASE_ADDR MRAM_MAX_SIZE>;
			erase-block-size = <2048>;
			write-block-size = <16>;
		};
	};

	/* DTCM */
	dtcm: dtcm@DTCM_BASE_NAME {
		compatible = "zephyr,memory-region";
		reg = <DTCM_BASE_ADDR DTCM_MAX_SIZE>;
		zephyr,memory-region = "DTCM";
	};

	/* SRAM */
	sram: memory@SSRAM_BASE_NAME {
		compatible = "mmio-sram";
		reg = <SSRAM_BASE_ADDR SSRAM_MAX_SIZE>;
	};

	xip0: memory@MSPI0_APERTURE_BASE_NAME {
		compatible = "zephyr,memory-region";
		reg = <MSPI0_APERTURE_BASE_ADDR MSPI0_APERTURE_MAX_SIZE>;
		zephyr,memory-region = "XIP0";
	};

	xip1: memory@MSPI1_APERTURE_BASE_NAME {
		compatible = "zephyr,memory-region";
		reg = <MSPI1_APERTURE_BASE_ADDR MSPI1_APERTURE_MAX_SIZE>;
		zephyr,memory-region = "XIP1";
	};

	xip2: memory@MSPI2_APERTURE_BASE_NAME {
		compatible = "zephyr,memory-region";
		reg = <MSPI2_APERTURE_BASE_ADDR MSPI2_APERTURE_MAX_SIZE>;
		zephyr,memory-region = "XIP2";
	};

