			compatible = "ambiq,usb";
			reg = <USB_REG_BASE USB_REG_SIZE>;
			interrupts = <27 0>;
			num-bidir-endpoints = <6>;
			maximum-speed = "high-speed";
			has-internal-usb-ldo;
			status = "disabled";
		};

