/dts-v1/;
#include <ambiq/ambiq_apollo510.dtsi>
#include "apollo510_eb-pinctrl.dtsi"

/ {
	model = "Ambiq Apollo510 Engineering Board";
	compatible = "ambiq,apollo510_eb";

	chosen {
		zephyr,itcm = &itcm;
		zephyr,dtcm = &dtcm;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
		zephyr,uart-pipe = &uart0;
		ambiq,xo32m = &xo32m_xtal;
		ambiq,xo32k = &xo32k_xtal;
		ambiq,extrefclk = &extrefclk;
	};

	aliases {
		watchdog0 = &wdt0;
		sdhc0 = &sdio0;
		sdhc1 = &sdio1;
	};
};

&itm {
	pinctrl-0 = <&swo_default>;
	pinctrl-names = "default";
};

&uart0 {
	current-speed = <115200>;
	pinctrl-0 = <&uart0_default>;
	pinctrl-names = "default";
	status = "okay";
};

&stimer0 {
	clk-source = <3>;
};

&wdt0 {
	status = "okay";
};

&sdio0 {
	pinctrl-0 = <&sdio0_default>;
	pinctrl-names = "default";
	txdelay = <0>;
	rxdelay = <0>;
	cd-gpios = <&gpio160_191 1 GPIO_ACTIVE_LOW>;
	status = "okay";
	mmc {
		compatible = "zephyr,mmc-disk";
        disk-name = "SD";
		status = "okay";
	};
};

&sdio1 {
	pinctrl-0 = <&sdio1_default>;
	pinctrl-names = "default";
	txdelay = <9>;
	rxdelay = <11>;
	cd-gpios = <&gpio160_191 15 GPIO_ACTIVE_LOW>;
	status = "okay";
};

zephyr_udc0: &usb {
	vddusb33-gpios = <&gpio64_95 27 (GPIO_PULL_UP)>;
	vddusb0p9-gpios = <&gpio64_95 26 (GPIO_PULL_UP)>;
	status = "okay";
};

&pdm0 {
	pinctrl-0 = <&pdm0_default>;
	pinctrl-names = "default";
	status = "okay";
};

&gpio64_95 {
	status = "okay";
};
